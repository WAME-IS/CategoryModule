services:
	ComponentManager:
		setup:
			- addComponent(Wame\CategoryModule\Vendor\Wame\ComponentModule\CategoryMenuComponent())

	PermissionObject:
		setup:
			- addResource('categoryMenu')
			- addResourceAction('categoryMenu', 'create')
			- allow('moderator', 'categoryMenu', 'create')
			- addResourceAction('categoryMenu', 'edit')
			- allow('moderator', 'categoryMenu', 'edit')
			- addResourceAction('categoryMenu', 'delete')
			- allow('admin', 'categoryMenu', 'delete')